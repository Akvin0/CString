1146 \par
1147         {\cf18 size_t} len = obj->length;\par
1148         {\cf18 size_t} pos = *start_pos;\par
1149 \par
1150         {\cf19 while} (pos < len && strchr(delimiters, obj->data[pos]) != NULL)\par
1151             pos++;\par
1152 \par
1153         {\cf19 if} (pos >= len)\par
1154         \{\par
1155             *start_pos = pos;\par
1156             cstr_unlock(obj);\par
1157             {\cf19 return} {\cf17 false};\par
1158         \}\par
1159 \par
1160         {\cf18 size_t} token_start = pos;\par
1161         {\cf18 size_t} token_end = invalid;\par
1162         {\cf18 bool} in_zone = {\cf17 false};\par
1163         {\cf18 char} zone_end = {\cf23 '\\0'};\par
1164         {\cf18 bool} escape = {\cf17 false};\par
1165 \par
1166         {\cf19 for} (; pos < len; pos++)\par
1167         \{\par
1168             {\cf18 char} c = obj->data[pos];\par
1169 \par
1170             {\cf19 if} (escape)\par
1171             \{\par
1172                 escape = {\cf17 false};\par
1173                 {\cf19 continue};\par
1174             \}\par
1175 \par
1176             {\cf19 if} (in_zone)\par
1177             \{\par
1178                 {\cf19 if} (c == zone_end)\par
1179                 \{\par
1180                     in_zone = {\cf17 false};\par
1181                     zone_end = {\cf23 '\\0'};\par
1182                 \}\par
1183             \}\par
1184             {\cf19 else}\par
1185             \{\par
1186                 {\cf19 if} (strchr(delimiters, c) != NULL)\par
1187                 \{\par
1188                     token_end = pos;\par
1189                     {\cf19 break};\par
1190                 \}\par
1191 \par
1192                 {\cf19 if} (zone_pairs)\par
1193                 \{\par
1194                     {\cf19 for} ({\cf18 int} z = 0; zone_pairs[z] != {\cf23 '\\0'}; z += 2)\par
1195                     \{\par
1196                         {\cf19 if} (zone_pairs[z + 1] == {\cf23 '\\0'})\par
1197                             {\cf19 break};\par
1198                         {\cf19 if} (c == zone_pairs[z])\par
1199                         \{\par
1200                             in_zone = {\cf17 true};\par
1201                             zone_end = zone_pairs[z + 1];\par
1202                             {\cf19 break};\par
1203                         \}\par
1204                     \}\par
1205                 \}\par
1206 \par
1207                 {\cf19 if} (escape_chars && strchr(escape_chars, c) != NULL)\par
1208                     escape = {\cf17 true};\par
1209             \}\par
1210         \}\par
1211 \par
1212         token_end = (pos == len) ? len : token_end;\par
1213 \par
1214         {\cf18 size_t} token_len = token_end - token_start;\par
1215         {\cf18 char}* temp = ({\cf18 char}*)malloc(token_len + 1);\par
1216         {\cf19 if} (!temp)\par
1217         \{\par
1218             cstr_unlock(obj);\par
1219             {\cf19 return} {\cf17 false};\par
1220         \}\par
1221 \par
1222         memcpy(temp, obj->data + token_start, token_len);\par
1223         temp[token_len] = {\cf23 '\\0'};\par
1224 \par
1225         {\cf19 if} (!cstr_create_from_chars(token, temp))\par
1226         \{\par
1227             free(temp);\par
1228             cstr_unlock(obj);\par
1229             {\cf19 return} {\cf17 false};\par
1230         \}\par
1231 \par
1232         free(temp);\par
1233         *start_pos = (token_end < len) ? token_end + 1 : len;\par
1234 \par
1235         cstr_unlock(obj);\par
1236 \par
1237         {\cf19 return} {\cf17 true};\par
1238     \}\par
